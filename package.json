{
  "name": "@porsche-design-system/fallbacks",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "assets",
    "partials"
  ],
  "scripts": {
    "start:assets": "npm run start --workspace=@porsche-design-system/assets",
    "build:assets": "npm run build --workspace=@porsche-design-system/assets",
    "test:unit:assets": "npm run test:unit --workspace=@porsche-design-system/assets",
    "test:vrt:assets": "npm run test:vrt --workspace=@porsche-design-system/assets",
    "test:smoke:unit:assets": "npm run test:smoke:unit --workspace=@porsche-design-system/assets",
    "build:partials": "npm run build --workspace=@porsche-design-system/partials",
    "test:partials": "npm run test --workspace=@porsche-design-system/partials",
    "npm:outdated": "npm outdated --workspaces",
    "npm:upgrade": "npx npm-check-updates --workspaces -u && npm install",
    "npm:remove": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
    "npm:clean": "npm cache clean --force"
  },
  "volta": {
    "node": "22.12.0"
  }
}